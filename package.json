{
  "name": "eslint-config-commercetools",
  "version": "6.0.0",
  "description": "Commercetools's eslint config, following our styleguide",
  "main": "index.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/commercetools/eslint-config.git"
  },
  "author": "Nicola Molinari <nicola.molinari@commercetools.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commercetools/eslint-config/issues"
  },
  "homepage": "https://github.com/commercetools/eslint-config",
  "peerDependencies": {
    "eslint": ">=3",
    "babel-eslint": ">=7",
    "eslint-config-airbnb": ">=14",
    "eslint-plugin-import": ">=2",
    "eslint-plugin-jsx-a11y": ">=3",
    "eslint-plugin-react": ">=6"
  },
  "files": [
    "index.js",
    "with-react.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "commercetools",
    "javascript",
    "styleguide"
  ],
  "scripts": {
    "lint": "./scripts/lint .",
    "test": "npm run lint",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "precommit": "./scripts/lint-staged-files"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "commitizen": "2.10.1",
    "eslint": "4.19.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-formatter-pretty": "1.3.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-react": "7.9.1",
    "husky": "0.14.3",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "validate-commit-msg": "2.14.0"
  }
}
